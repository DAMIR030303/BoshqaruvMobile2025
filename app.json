{
  "expo": {
    "name": "BoshqaruvMobile",
    "slug": "boshqaruvmobile",
    "version": "1.0.0",
    "description": "Boshqaruv tizimi uchun zamonaviy mobil ilova",
    "author": "Damir Nurmurodov",
    "sdkVersion": "51.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.damirnurmurodov.boshqaruvmobile",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Ilova kamera ruxsatini so'raydi",
        "NSPhotoLibraryUsageDescription": "Ilova rasmlar galereyasiga kirish ruxsatini so'raydi",
        "NSLocationWhenInUseUsageDescription": "Ilova joylashuv ma'lumotlarini olish ruxsatini so'raydi"
      }
    },
    "android": {
      "package": "com.damirnurmurodov.boshqaruvmobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "boshqaruvmobile-2025"
      }
    },
    "plugins": [
      "expo-router",
      "expo-camera",
      "expo-image-picker",
      "expo-location",
      "expo-notifications"
    ]
  }
}